{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.Folder}}{{if .Root}} as root{{end}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\ue70f ",
          "foreground": "#016fb9",
          "type": "status",
          "foreground_templates": [
            "{{ if .Error }}#ce4937{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\udb84\ude38",
          "foreground": "#191b1c",
          "background": "#b8b814",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "leading_diamond": "\uf178 ",
          "template": " {{ .Path }} ",
          "foreground": "#afa98d",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": true,
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue73c {{ if .Venv }}{{ .Venv }} {{ end }}",
          "foreground": "#191b1c",
          "background": "#3776ab",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "add_icon": "\uf916 ",
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cache_duration": "none",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\uf113 ",
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }} ",
          "foreground": "#191b1c",
          "powerline_symbol": "\ue0b0",
          "background": "#14b814",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#b8b814{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#8619e6{{ end }}",
            "{{ if gt .Ahead 0 }}#8619e6{{ end }}",
            "{{ if gt .Behind 0 }}#8619e6{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
