{
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "blue",
                    "foreground_templates": [
                        "{{ if .Error }}#ce4937{{ end }}"
                    ],
                    "template": "\ue70f ",
                    "type": "status",
                    "properties": {
                        "always_enabled": true
                    }
                },
                {
                    "type": "root",
                    "style": "diamond",
                    "leading_diamond": " \ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "foreground": "#191b1c",
                    "background": "#b8b814",
                    "template": "\udb84\ude38"
                },
                {
                    "foreground": "magenta",
                    "leading_diamond": "\uf178 ",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "powerline",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "type": "python",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4 ",
                    "properties": {
                        "display_mode": "environment",
                        "fetch_virtual_env": true,
                        "display_default": true,
                        "home_enabled": true
                    },
                    "foreground": "#191b1c",
                    "background": "#3776ab",
                    "template": "\ue73c {{ if .Venv }}{{ .Venv }} {{ end }}"
                },
                {
                    "background": "#14b814",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#b8b814{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#8619e6{{ end }}",
                        "{{ if gt .Ahead 0 }}#8619e6{{ end }}",
                        "{{ if gt .Behind 0 }}#8619e6{{ end }}"
                    ],
                    "foreground": "#191b1c",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "add_icon": "\uf916 ",
                        "github_icon": "\uf113 ",
                        "branch_ahead_icon": "\u2191",
                        "branch_behind_icon": "\u2193",
                        "branch_gone": "\u2262",
                        "branch_icon": "",
                        "branch_identical_icon": "",
                        "cherry_pick_icon": "\u2713 ",
                        "commit_icon": "\u25b7 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "merge_icon": "\u25f4 ",
                        "no_commits_icon": "",
                        "rebase_icon": "\u2c62 ",
                        "tag_icon": "\u25b6 "
                    },

                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt",
            "vertical_offset": 0
        }
    ],
    "console_title_template": "{{.Folder}}{{if .Root}} as root{{end}}",
    "final_space": true,
    "osc99": true,
    "version": 2
}
